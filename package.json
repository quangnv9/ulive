{
  "name": "ulive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "cross-env craco start",
    "start:dev": "cross-env REACT_APP_STAGE=dev craco start",
    "start:test": "cross-env REACT_APP_STAGE=test craco start",
    "start:staging": "cross-env REACT_APP_STAGE=staging craco start",
    "start:prod": "cross-env REACT_APP_STAGE=production craco start",
    "build": "cross-env craco build",
    "build:dev": "cross-env REACT_APP_STAGE=dev craco build",
    "build:test": "cross-env REACT_APP_STAGE=test craco build",
    "build:staging": "cross-env REACT_APP_STAGE=staging craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "tsc --noEmit && eslint --fix \"src/**/*.{ts,tsx}\"",
    "pre-commit": "npm run prettier && npm run lint",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@craco/craco": "^6.2.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.16.13",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.4",
    "clsx": "^1.1.1",
    "craco-less": "^1.20.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.23.0",
    "jwt-decode": "^3.1.2",
    "postcss": "^7.0.35",
    "postinstall": "^0.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.23.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.38.1",
    "source-map-explorer": "^2.5.2",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "husky": "^7.0.2",
    "inline-assets": "^1.4.8",
    "jest-junit": "^12.2.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "ulive"
  },
  "author": "",
  "license": "ISC"
}
